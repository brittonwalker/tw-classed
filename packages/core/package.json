{
  "name": "@tw-classed/core",
  "version": "1.0.0-alpha.1",
  "description": "A Stitches & Styled-Components inspired library to create reusable Tailwind react components",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./parser": {
      "import": "./dist/parser.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "Sanna Jammeh",
  "license": "MIT",
  "homepage": "https://github.com/sannajammeh/tw-classed",
  "repository": {
    "type": "git",
    "url": "https://github.com/sannajammeh/tw-classed"
  },
  "devDependencies": {
    "@jest/core": "^29.3.1",
    "@types/jest": "^29.2.2",
    "@vitest/ui": "^0.25.1",
    "c8": "^7.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.1"
  }
}
